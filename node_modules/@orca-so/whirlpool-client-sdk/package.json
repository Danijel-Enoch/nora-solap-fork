    "@types/decimal.js": "^7.4.0",
    "@types/mocha": "^8.2.2",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "chai": "^4.3.4",
    "eslint-config-prettier": "^8.3.0",
    "mocha": "~9.0.3",
    "prettier": "^2.3.2",
    "tsup": "^5.12.1",
    "typescript": "4.6.2"
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
