    "bn.js": "~5.2.0",
    "decimal.js": "~10.3.1"
  },
  "peerDependencies": {
    "@solana/web3.js": ">=1.15.0"
  },
  "devDependencies": {
    "@metaplex-foundation/mpl-token-metadata": "~1.2.5",
